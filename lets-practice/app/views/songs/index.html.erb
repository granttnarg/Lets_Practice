<div class="row show-page">
</div>

<div class="container">

  <% @songs.each do |song| %>
    <%= link_to song_path(song) do %>
      <div class="card-product index-card">
        <%= cl_image_tag song.photo, height: 200, crop: :fill, id: "hover-image" %>

        <div class="card-product-infos">
          <h2><%= song.artist%></h2>
          <p><%= song.name%> // <%= song.native_bpm%> BPM // <%= song.key %> </p>
        </div>
      </div>
    <% end %>
  <% end %>
</div>



